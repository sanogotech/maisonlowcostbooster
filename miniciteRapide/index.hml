  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
  <script>
    // Initialize sliders
    document.getElementById('terrainCount').addEventListener('input', function() {
      document.getElementById('terrainCountValue').textContent = this.value;
    });
    
    document.getElementById('constructionPercent').addEventListener('input', function() {
      document.getElementById('constructionPercentValue').textContent = this.value + '%';
    });
    
    document.getElementById('rentalRatio').addEventListener('input', function() {
      document.getElementById('rentalRatioValue').textContent = this.value + '%';
    });
    
    // Calculate ROI function
    function calculateROI() {
      const terrainCount = parseInt(document.getElementById('terrainCount').value);
      const constructionPercent = parseInt(document.getElementById('constructionPercent').value);
      const rentalRatio = parseInt(document.getElementById('rentalRatio').value);
      
      // Calculation logic based on the provided data
      const terrainsToSell = Math.floor(terrainCount * (100 - constructionPercent) / 100);
      const terrainsToBuild = terrainCount - terrainsToSell;
      
      // Revenue from selling empty terrains
      const revenueFromEmptyTerrains = terrainsToSell * 7000000; // 7M average per terrain
      
      // Investment in construction
      const constructionInvestment = terrainsToBuild * 28500000; // ~28.5M per built terrain
      
      // Revenue from built properties (mix of sales and rentals)
      const builtRevenue = terrainsToBuild * 40000000; // ~40M average per built terrain
      
      // Total investment
      const terrainInvestment = terrainCount * 4000000; // 4M per terrain
      const totalInvestment = terrainInvestment + constructionInvestment;
      
      // Total revenue
      const totalRevenue = revenueFromEmptyTerrains + builtRevenue;
      
      // Net profit
      const netProfit = totalRevenue - totalInvestment;
      
      // ROI
      const roi = (netProfit / totalInvestment) * 100;
      
      // Update the UI
      document.getElementById('totalInvestment').textContent = formatCFA(totalInvestment);
      document.getElementById('totalRevenue').textContent = formatCFA(totalRevenue);
      document.getElementById('netProfit').textContent = formatCFA(netProfit);
      document.getElementById('roiValue').textContent = roi.toFixed(1) + '%';
      
      // Show results
      document.getElementById('simulationResults').style.display = 'block';
    }
    
    // Format CFA currency
    function formatCFA(amount) {
      return new Intl.NumberFormat('fr-FR', { style: 'currency', currency: 'XOF' }).format(amount);
    }
  </script>
